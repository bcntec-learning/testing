<?xml version="1.0" encoding="UTF-8"?>
<configuration>


    <appender name="selenium" class="ch.qos.logback.core.ConsoleAppender">
        <Target>System.out</Target>
        <encoder>
            <pattern>%m%n</pattern>
        </encoder>
    </appender>
    <root level="INFO">
        <appender-ref ref="selenium"/>
    </root>


</configuration>